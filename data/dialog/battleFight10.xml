<?xml version="1.0" encoding="UTF-8"?>
<!--
        bg:背景类型，根据bg值选择bg的class,【1】静态图背景【2】黑色背景
        bgUrl:背景图片地址，如果没有，可以写""
        leftSpeaker:左边说话人物的头像
        leftSpeakerPos:左边说话人物的中心点坐标
        rightSpeaker/rightSpeakerPos    右边
        nameTf:显示名字文本框的x/y/width
        nameContent:显示说话人的姓名
        speakerTf：说话内容文本框的x/y/width
        speakerContent：说话内容
        light【0】两边都不亮【1】左边亮【2】右边亮
-->
<root>
    <dialog id="011001start" bg="1" bgUrl="miaoTang.jpg">
        <subDialog leftSpeaker="sufei.png" leftSpeakerPos="500,400"
                   rightSpeaker="caocao.png" rightSpeakerPos="1500,400"
                   nameTf="750,450,400" nameContent="苏飞"
                   speakerTf="600,500,300" speakerContent="臣奉荆州牧之命，向陛下献礼，以尽人臣之义。"
                   light="1"
                />
        <subDialog nameContent="曹操" speakerContent="陛下，臣建议加封他为楚侯，以正君臣名分！" light="2" />
		<subDialog leftSpeaker="liuxie.png" leftSpeakerPos="500,400"
		           nameContent="刘协" speakerContent="传朕旨意，加封荆州牧为楚侯，赐白玉一双。" light="1" />
    </dialog>
