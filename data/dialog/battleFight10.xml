		<subDialog nameContent="{myname}" speakerContent="可惜没有能和兄长并肩作战，这是我最大的遗憾。" light="1" />
		<subDialog nameContent="赵云" speakerContent="等我解掉最后一个心结后，或许我会和你并肩作战。" light="2" />
		
    </dialog>
	<dialog id="010110end" bg="1" bgUrl="jiaLi.jpg">
        <subDialog leftSpeaker="jiaxu.png" leftSpeakerPos="500,400"
                   rightSpeaker="" rightSpeakerPos="1500,400"
                   nameTf="750,450,400" nameContent="旁白"
