<?xml version="1.0" encoding="UTF-8"?>
<preludes>
    <!--
        type:场景类型,根据type确定prlude的class，【1】SubPrelude【2】BlackPrelude【3】SpeakerPrelude【4】WhereAmIPrelude【5】GongChengPrelude【6】YueBingPrelude【7】baoxinprelude
        bg:背景类型，根据bg值选择bg的class,【1】静态图背景【2】黑色背景
        bgUrl:背景图片地址，如果没有，可以写""
        leftSpeaker:左边说话人物的头像
        leftSpeakerPos:左边说话人物的中心点坐标
        rightSpeaker/rightSpeakerPos    右边
        scene:播放prelude的场景。【null或者“”】preludescene【fight】战斗场景【main】Mainscene
        step:新手引导步骤，用来记录和开始步骤,如果小于0则不需要记录。如果大于0需要记录
    -->
    <!--subTitle
        time:展示时间(如果有声音，以声音结束为准，没有以time的值为准)
        content:说话内容
        light【0】两边都不亮【1】左边亮【2】右边亮
        displayName 说话人的名字
        sound:播放的声音
        bg:具体说某句话时的背景。如果有，需要preludebg更换背景。没有，则不更换
    -->
    <preludeItem id="1" type="5" name="liubei" step="1" bg="1" bgUrl="" >
        <subTitle time="1" content="汉建安六年，汝南之战爆发，" sound="gongcheng_001"></subTitle>
        <subTitle time="1" content="刘备收编数万黄巾军，向许都进发，" sound="gongcheng_002"></subTitle>
        <subTitle time="1" content="企图趁曹操在河北作战的机会偷袭许都。" sound="gongcheng_003"></subTitle>
    </preludeItem>
    <preludeItem id="2" type="6" name="liubei" step="2" bg="1" bgUrl="" >
        <subTitle time="1" content="此时，曹操已经结束了仓亭之战，" sound="yuebing_001"></subTitle>
        <subTitle time="1" content="收兵回许都，亲率大军南击刘备，" sound="yuebing_002"></subTitle>
        <subTitle time="1" content="曹操发誓，这次一定要将刘备彻底消灭。" sound="yuebing_003"></subTitle>
    </preludeItem>
    <preludeItem id="3" type="4" name="liubei" step="3" bg="1" bgUrl="" >
        <subTitle time="1" content="你们怎么让哥掉进山沟了？" sound="huanxing_001" ></subTitle>
        <subTitle time="1" content="…… 我这是在哪里？" sound="huanxing_002" ></subTitle>
        <subTitle time="1" content="哥，你醒了。" sound="huanxing_003"></subTitle>
        <subTitle time="1" content="不好，曹军抓壮丁来了！" sound="huanxing_004" bg="assets/images/backgroud/dialog/zhengBing.jpg"></subTitle>
    </preludeItem>
	<preludeItem id="4" type="7" name="liubei" step="4" bg="1" bgUrl="" >
        <subTitle time="1" content="将军，前方就是汝南黄巾军主力。" sound="baoxin_001" ></subTitle>
        <subTitle time="1" content="让民夫在前面当炮灰，大军跟进。" sound="baoxin_002"></subTitle>
        <subTitle time="1" content="得令！" sound="baoxin_003" ></subTitle>
    </preludeItem>
	<preludeItem id="5" type="1" name="liubei" step="5" bg="1" bgUrl="" >
        <subTitle time="1" content="这里没人，赶紧走。" sound="taopao_001" ></subTitle>
        <subTitle time="1" content="我们逃出来了。" sound="taopao_002"></subTitle>
        <subTitle time="1" content="哥，以后就靠你了。" sound="taopao_003" ></subTitle>
        <subTitle time="1" content="放心，倩儿。阿牛，我们先找个安全的地方。" sound="taopao_004" ></subTitle>
    </preludeItem>
